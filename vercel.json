{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "outputDirectory": "build" }
      },
      {
        "src": "manage.py",
        "use": "@natanffs/vercel-python-wsgi",
        "config": { "output": "build" }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/build/manage.py" }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "stenograph.settings"
    }
  }
  